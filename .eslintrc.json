{
  "extends": ["next/core-web-vitals"],
  "plugins": ["unused-imports", "@typescript-eslint"],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_"
      }
    ],
    "unused-imports/no-unused-imports": "error"
  }
}
